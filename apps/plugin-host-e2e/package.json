{
  "name": "@modu-nest/plugin-host-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "plugin-host-e2e",
    "implicitDependencies": [
      "plugin-host"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/plugin-host-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "plugin-host:build",
          "plugin-host:serve"
        ]
      }
    }
  }
}
