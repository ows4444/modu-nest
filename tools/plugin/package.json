{
  "name": "@tools/plugin",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "tools/plugin/dist",
          "main": "tools/plugin/src/index.ts",
          "tsConfig": "tools/plugin/tsconfig.lib.json",
          "rootDir": "tools/plugin/src",
          "generatePackageJson": false,
          "assets": [
            {
              "input": "./tools/plugin/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./tools/plugin/src",
              "glob": "**/*.d.ts",
              "output": "."
            },
            {
              "input": "./tools/plugin",
              "glob": "generators.json",
              "output": "."
            },
            {
              "input": "./tools/plugin",
              "glob": "executors.json",
              "output": "."
            },
            {
              "input": "./tools/plugin/src/generators",
              "glob": "files/**/*",
              "output": "./generators"
            }
          ]
        }
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{workspaceRoot}/coverage/{projectRoot}"
        ],
        "options": {
          "jestConfig": "tools/plugin/jest.config.ts"
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": [
          "{options.outputFile}"
        ],
        "options": {
          "lintFilePatterns": [
            "tools/plugin/**/*.ts"
          ]
        }
      },
      "typecheck": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsc --noEmit --project tools/plugin/tsconfig.json"
        }
      }
    }
  },
  "dependencies": {
    "@nx/devkit": "21.4.0",
    "tslib": "^2.3.0"
  },
  "generators": "./generators.json",
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "generators.json",
    "executors.json"
  ],
  "executors": "./executors.json"
}
