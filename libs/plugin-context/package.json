{
  "name": "@libs/plugin-context",
  "version": "1.1.0",
  "private": true,
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@nestjs/common": "^11.0.0",
    "@libs/plugin-types": "1.0.0",
    "@libs/plugin-core": "1.1.0"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/plugin-context",
          "tsConfig": "libs/plugin-context/tsconfig.lib.json",
          "packageJson": "libs/plugin-context/package.json",
          "main": "libs/plugin-context/src/index.ts",
          "assets": [
            "libs/plugin-context/*.md"
          ]
        }
      }
    }
  }
}
